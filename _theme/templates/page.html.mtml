<!DOCTYPE html>
<html lang="ja">

<head>
    <mt:Include module="Head">
    <title><mt:PageTitle></title>
</head>

<body>
    <mt:Include module="Navigation">

    <!-- Page Header -->
    <!-- Set your background image for this header on the line below. -->
    <header class="intro-header" style="background-image: url('img/home-bg.jpg')">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                    <div class="page-heading">
                        <h1>blog</h1>
                        <hr class="small">
                        <span class="subheading"><mt:PageTitle></span>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <!-- Main Content -->
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                <mt:PageBody>
                <mt:PageMore>
                <!-- Pager -->
                <ul class="pager">
                    <li><a href="<mt:BlogURL>">index</a></li>
                </ul>
            </div>
        </div>
    </div>

    <hr>

    <mt:Include module="Footer">
    <mt:Include module="Scripts">
</body>

</html>
